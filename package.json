  "main": "src/index.js",
  "dependencies": {
    "compression-webpack-plugin": "1.1.12",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-scripts": "1.1.4",
    "rimraf": "2.6.2",
    "uglifyjs-webpack-plugin": "1.3.0",
